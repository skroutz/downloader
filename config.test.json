{
	"redis": {
		"addr": "127.0.0.1:6379"
	},
	"api": {
		"heartbeat_path":"/tmp/disable_api"
	},
	"processor": {
		"download_url": "http://localhost/foo",
		"storage_dir":"/tmp/",
		"request_headers": {
			"Accept": "*/*",
			"User-Agent": "Downloader v1"
		},
		"stats_interval": 300
	},
	"notifier": {
		"concurrency": 10,
		"stats_interval": 300,
		"deletion_interval": 1
	},
	"backends": {
		"http": {
			"timeout": 30
		}
	}
}
